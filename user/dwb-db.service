[Unit]
Description=dwb cookie database
StopWhenUnneeded=true

Requires=mount-priv.service
After=mount-priv.service

# dwb cookies are under ~/priv
Requisite=synced.target
After=synced.target


[Service]
Type=oneshot
RemainAfterExit=true

SyslogIdentifier=dwb-db
ExecStart=/usr/bin/true
ExecStop=/usr/bin/systemctl --user --no-block start commit-dwb.service
