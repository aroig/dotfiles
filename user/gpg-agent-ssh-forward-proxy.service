[Unit]
Description=ssh agent remote proxy

Requires=gpg-agent-ssh-forward-proxy.socket
After=gpg-agent-ssh-forward-proxy.socket

Requires=gpg-agent.service
After=gpg-agent.service

[Service]
Type=simple
SyslogIdentifier=ssh-agent-proxy
ExecStart=/usr/lib/systemd/systemd-socket-proxyd %h/.gnupg/S.gpg-agent.shh

# ExecStart=/usr/bin/socat TCP-LISTEN:11001,bind=127.0.0.1 UNIX-CONNECT:%h/.gnupg/S.gpg-agent.ssh
